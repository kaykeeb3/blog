generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       String @id @default(uuid())
  name     String
  email    String @unique
  password String
}

model News {
  id          String   @id @default(uuid())
  title       String
  description String
  createdAt   DateTime @default(now())
  area        String
}
